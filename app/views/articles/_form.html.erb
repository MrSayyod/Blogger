<%= form_for(@article) do |f| %>
    <ul>
      <% @article.errors.full_messages.each do |error| %>
        <li> <%= error %> </li>
      <% end %>
    </ul>
    <p>
      <% f.label :title %> <br>
      <%= f.text_field :title %> <br>
    </p>
    <p>
      <% f.label :body %> <br>
      <%= f.text_area :body %> <br>
    </p>
    <p>
      <%= f.submit %> <br>
    </p>
  <% end %>